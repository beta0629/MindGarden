{
    "java.home": "/opt/homebrew/opt/openjdk@17",
    "java.jdt.ls.java.home": "/opt/homebrew/opt/openjdk@17",
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -Xmx4g -Xms1g",
    "java.import.maven.enabled": true,
    "java.autobuild.enabled": true,
    "java.compile.nullAnalysis.mode": "automatic",
    "java.lombokSupport.enabled": true,
    "java.trace.server": "verbose",
    "problems.showCurrentInStatus": true,
    "editor.renderValidationDecorations": "on",
    "problems.decorations.enabled": true,
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.project.importOnFirstTimeStartup": "automatic",
    "java.configuration.checkProjectSettingsExclusions": false,
    "java.project.sourcePaths": [
        "src/main/java"
    ],
    "java.project.outputPath": "target/classes"
}