{
  "name": "MindGardenMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build:android:debug": "cd android && ./gradlew assembleDebug",
    "build:android:release": "cd android && ./gradlew assembleRelease",
    "build:ios:debug": "cd ios && xcodebuild -workspace MindGarden.xcworkspace -scheme MindGarden -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
    "build:ios:release": "cd ios && xcodebuild -workspace MindGarden.xcworkspace -scheme MindGarden -configuration Release -sdk iphoneos -derivedDataPath ios/build",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && rm -rf build",
    "clean": "npm run clean:android && npm run clean:ios",
    "postinstall": "patch-package || echo 'patch-package not found, skipping'",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios/"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-picker/picker": "^2.11.4",
    "@react-native-seoul/kakao-login": "^5.4.2",
    "@react-native-seoul/naver-login": "^4.2.2",
    "@react-native/new-app-screen": "0.82.1",
    "@react-navigation/bottom-tabs": "^7.7.2",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/stack": "^7.6.1",
    "axios": "^1.13.1",
    "lucide-react-native": "^0.548.0",
    "react": "19.1.1",
    "react-native": "0.82.1",
    "react-native-calendars": "^1.1313.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "^2.29.0",
    "react-native-image-crop-picker": "^0.51.1",
    "react-native-image-picker": "^8.2.1",
    "react-native-image-resizer": "^1.4.5",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-permissions": "^5.4.3",
    "react-native-reanimated": "^3.19.3",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-svg": "^15.14.0",
    "react-native-toast-message": "^2.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native-firebase/app": "^23.5.0",
    "@react-native-firebase/messaging": "^23.5.0",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/eslint-config": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.1",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "patch-package": "^8.0.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
